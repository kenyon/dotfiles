! With tabbed rxvt-unicode, cant scroll with keyboard unless mouse cursor is
! in the window. Stupid.
URxvt.perl-ext-common: default,matcher
!,tabbed

URxvt.font: xft:DejaVu Sans Mono:size=10
URxvt.saveLines: 10000
URxvt.loginShell: true
URxvt.background: black
URxvt.foreground: white
URxvt.jumpScroll: true
URxvt.skipScroll: true
!URxvt.fading: 20
URxvt.urgentOnBell: true
URxvt.scrollstyle: plain
URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: false
URxvt.scrollTtyKeypress: true
URxvt.pointerBlank: true

!UXTerm*backarrowKey: false
!UXTerm*backarrowKeyIsErase: true
!UXTerm*VT100.titeInhibit:  true
UXTerm*deleteIsDEL: true

UXTerm*highlightSelection:  true
UXTerm*scrollBar: true

UXTerm*VT100.loginShell: true
UXTerm*VT100.rightScrollBar: true
UXTerm*VT100.saveLines: 10000
UXTerm*VT100.trimSelection: true
UXTerm*VT100.visualBell: true
UXTerm*VT100.visualBellDelay: 20

UXTerm*VT100.background: black
UXTerm*VT100.colorBDMode: on
UXTerm*VT100.colorMode:  on
UXTerm*VT100.colorULMode:  on
UXTerm*VT100.cursorColor: lime green
UXTerm*VT100.dynamicColors:  on
UXTerm*VT100.eightBitInput:  true
UXTerm*VT100.eightBitOutput:  true
UXTerm*VT100.foreground: white
UXTerm*VT100.underLine:  off

UXTerm*VT100.color0: dark slate grey
UXTerm*VT100.color1: red3
UXTerm*VT100.color2: green3
UXTerm*VT100.color3: yellow3
UXTerm*VT100.color4: blue3
UXTerm*VT100.color5: magenta3
UXTerm*VT100.color6: cyan3
UXTerm*VT100.color7: gray90
UXTerm*VT100.color8: gray30
UXTerm*VT100.color9: red
UXTerm*VT100.color10: green
UXTerm*VT100.color11: yellow
UXTerm*VT100.color12: blue
UXTerm*VT100.color13: magenta
UXTerm*VT100.color14: cyan
UXTerm*VT100.color15: white
UXTerm*VT100.colorUL: yellow
UXTerm*VT100.colorBD: white
